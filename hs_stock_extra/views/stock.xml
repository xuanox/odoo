<odoo>
	<data>


		<!-- model.name form view -->
		<record id="stock_picking_tree_inherit1" model="ir.ui.view">
			<field name="name">stock.picking.tree.inherit1</field>
			<field name="model">stock.picking</field>
			<field name="inherit_id" ref="stock.vpicktree"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='origin']" position="before">
					<field name="hs_managment" attrs="{'invisible': [('hs_managment', '=', '')]}"/>
				</xpath>
			</field>
		</record>


	</data>
</odoo>