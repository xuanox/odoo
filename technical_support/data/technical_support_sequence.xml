<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for Maintenance orders -->
        <record id="sequence_technical_support_order" model="ir.sequence">
            <field name="name">Technical Support Order</field>
            <field name="code">technical_support.order</field>
            <field name="prefix">TSO</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sequences for Work orders -->
        <record id="sequence_technical_support_workorder" model="ir.sequence">
            <field name="name">Technical Support Work order</field>
            <field name="code">technical_support.workorder</field>
            <field name="prefix">TSWO</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sequences for Maintenance request -->
        <record id="sequence_technical_support_request" model="ir.sequence">
            <field name="name">Technical Support Request</field>
            <field name="code">technical_support.request</field>
            <field name="prefix">TSR</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
